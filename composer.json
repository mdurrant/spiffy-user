{
    "name": "spiffy/spiffy-user",
    "authors": [
        {
            "name": "Kyle Spraggs",
            "email": "theman@spiffyjr.me",
            "homepage": "http://www.spiffyjr.me/"
        }
    ],
    "require": {
        "php": ">=5.4",
        "doctrine/common": "2.*",
        "doctrine/doctrine-orm-module": "0.8.0-beta4",
        "zendframework/zend-authentication": "~2.2",
        "zendframework/zend-cache": "~2.2",
        "zendframework/zend-code": "~2.2",
        "zendframework/zend-config": "~2.2",
        "zendframework/zend-console": "~2.2",
        "zendframework/zend-crypt": "~2.2",
        "zendframework/zend-form": "~2.2",
        "zendframework/zend-modulemanager": "~2.2",
        "zendframework/zend-mvc": "~2.2",
        "zendframework/zend-serializer": "~2.2",
        "zendframework/zend-version": "~2.2"
    },
    "require-dev": {
        "phpunit/phpunit": "3.*",
        "satooshi/php-coveralls": "dev-master",
        "spiffy/spiffy-test": "dev-master",
        "zendframework/zend-config": "~2.2",
        "zendframework/zend-i18n": "~2.2",
        "zendframework/zend-session": "~2.2",
        "zendframework/zend-test": "~2.2"
    },
    "autoload": {
        "psr-0": {
            "SpiffyUser": "src/",
            "SpiffyUserTest": "test/"
        }
